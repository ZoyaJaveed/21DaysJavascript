
// Remember, the possible candidates are: Tim, Sally, and Beth (in that order)
const castVote = (name, votes) => {
  // Code here!
  if (name === "Tim"){
    votes[0] ++;
  }else if (name === "Sally"){
    votes[1] ++;
  }else if(name === "Beth"){
    votes[2] ++;
  }else{
    console.log("Name does not exist");
  }
  // Remember to return a value!
  return votes;
}
