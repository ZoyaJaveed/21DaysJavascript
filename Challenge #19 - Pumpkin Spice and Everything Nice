
const pumpkinSpice = money => {
    // array of pies, lattes, macarons, total grams
  const pumpkinAmount = [0, 0, 0, 0];
  
  while (money >= 5) {
    pumpkinAmount[0]++;
    pumpkinAmount[3] += 30;
    money -= 5;
  }
  
  while (money >= 3) {
    pumpkinAmount[1]++;
    pumpkinAmount[3] += 15;
    money -= 3;
  }
  
  while (money >= 1) {
    pumpkinAmount[2]++;
    pumpkinAmount[3] += 3;
    money -= 1;
  }
  
  return pumpkinAmount;
  
}
